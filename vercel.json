{
  "version": 2,
  "builds": [
    {
      "src": "flavour/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "flavour/wsgi.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.13"
  }
}
